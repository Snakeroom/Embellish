{
	"compilerOptions": {
		"downlevelIteration": true,
		"esModuleInterop": true,
		"lib": [
			"es2020",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./dist",
		"resolveJsonModule": true,
		"rootDir": "./src",
		"skipLibCheck": true,
		"target": "es2015"
	}
}